  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0"
  },
  "dependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.29",
    "@types/ws": "^8.5.3",
    "dotenv": "^16.0.0",
    "eris": "github:abalabahaha/eris",
    "mongoose": "^6.3.2",
    "ms": "^2.1.3",
    "pretty-ms": "^7.0.1",
    "vulkava": "^1.2.4",
    "ws": "^8.6.0"
  }
